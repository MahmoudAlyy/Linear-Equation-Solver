function [s] = gauss_seidel(a,b,input)

a;
b;
iter = 2;
x=input;
old_x = input
%x=[];
n = size(a);
n = n(1);

%for i = 1:n
%x(i) = 0;
%end

for it = 1:iter
for i = 1:n
     
    sum=0;
    for j = 1:n
        if i ~= j
        sum = sum + a(i,j)*x(j);
        end
    end
    x(i) = (b(i)-sum) /a(i,i);
   
    
end
end
x